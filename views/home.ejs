<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Belajar server & API</title>
</head>
<body>
    <ul>
        <% cryptoData.forEach(function(crypto) { %>
            <li><%= crypto.code %> - <%= crypto.name %>:
                <% if(crypto.is_crypto) { %>
                    ✅
                <% } else { %>
                    ❌
                <% } %>
            </li>
        <% }); %>
    </ul>
</body>
</html>